// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url  	= "file:./dev.db"
}

model versions {
  id Int @id @default(autoincrement())
  tbversion String
  rversion String
  url String
}

model aircraft {
  id Int @id @default(autoincrement())
  msn Int @unique
  tail Int @unique
}

model switches {
  id Int @id @default(autoincrement())
  name String?
  atareference String
}